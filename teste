    Values = {"Spooky", "Defense", "Alabasto", "Clover", "Tower", "SpookyDefense", "JJCorridor", "RagnarokRaid"},
    Default = "SpookyDefense",
    Multi = false,
    Callback = function(value)
        selectedRaid = value
    end
})

