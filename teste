    while CandyUp.Value do
        -- Atualiza as informa칞칫es no Paragraph a cada 1 segundo
        updateParagraph()

        -- Verifica e dispara o evento remoto para CandyUp
        if Remote then
            Remote:FireServer(unpack(candyArgs))
        end

        wait(10) -- Atualiza a cada 1 segundo
    end
end)

