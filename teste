-- Fun칞칚o para aplicar o efeito de cor RGB apenas ao nome
local function applyRGBEffect()
    local hue = 0
    while wait(0.1) do
        hue = (hue + 1) % 360
        local color = Color3.fromHSV(hue / 360, 1, 1)
        if textLabel then
            textLabel.TextColor3 = color
        end
    end
end

-- Inicia a anima칞칚o RGB apenas no nome
