RankUp:OnChanged(function()
    while RankUp.Value do
        if Remote then
            for i = 1, 8 do
                args[5] = "Reward" .. i
                Remote:FireServer(unpack(args))
                wait(1) -- Aguarda 1 segundo entre cada envio
            end
            wait(30) -- Aguarda 30 segundos antes de reiniciar o processo
        end
    end
end)


